{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "interface-name": false,
    "no-any": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prefer-array-literal": [
      true,
      {
        "allow-type-parameters": true
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}